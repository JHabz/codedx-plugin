<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core">
    <j:if test="${from.showTablesAndCharts() &amp;&amp; from.hasValidResults()}">
        <div align="right">
            <p/>
            <div class="test-trend-caption">
                Code Dx Findings Trend (Severities)
            </div>
            <div>
                <img src="${from.urlName}/severityTrend"/>
            </div>
            <hr/>
            <div class="test-trend-caption">
                Code Dx Findings Trend (Status)
            </div>
            <div>
                <img src="${from.urlName}/statusTrend"/>
            </div>
        </div>
    </j:if>
</j:jelly>